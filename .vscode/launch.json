{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug",
      "type": "gdb",
      "gdbpath": "/usr/bin/rust-gdb",
      "request": "launch",
      "target": "/usr/bin/pkcs11-tool",
      "arguments": "--module target/debug/libnethsm_pkcs11.so -O",
      "env": {
        "P11NETHSM_CONFIG_FILE": "./p11nethsm.conf",
        "RUST_LOG": "trace"
      },
      "cwd": "${workspaceRoot}",
      "valuesFormatting": "prettyPrinters"
    },
    {
      "name": "Debug-Nginx",
      "type": "gdb",
      "gdbpath": "/usr/bin/rust-gdb",
      "request": "launch",
      "target": "/home/sautax/Downloads/nginx-1.25.2/objs/nginx",
      "arguments": "-c ${workspaceRoot}/_nginx.conf -g 'daemon off;'",
      "env": {
        "P11NETHSM_CONFIG_FILE": "./p11nethsm.conf",
        "RUST_LOG": "trace",
        "OPENSSL_CONF": "${workspaceRoot}/_openssl.cnf",
        "LD_LIBRARY_PATH": "/home/sautax/git/openssl",
      },
      "cwd": "${workspaceRoot}",
      "valuesFormatting": "prettyPrinters",
    }
  ]
}